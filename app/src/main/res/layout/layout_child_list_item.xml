<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"

    android:gravity="center"
    android:paddingBottom="@dimen/padding_10"
    android:paddingRight="@dimen/padding_10"
    android:paddingTop="@dimen/padding_10"
    android:orientation="vertical">

    <TextView
        android:id="@+id/icon_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="32dp"
        android:text="dklsfdsf"
        android:visibility="gone"
        android:textColor="@color/colorPrimary" />

    <TextView
        android:id="@+id/child_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_toRightOf="@id/icon_text_view"
        android:drawablePadding="@dimen/margin_16"
        android:gravity="center_vertical"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_medium" />



    <TextView
        android:id="@+id/bike_in"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_toRightOf="@id/icon_text_view"
        android:drawablePadding="@dimen/margin_16"
        android:gravity="center_vertical"
        android:text="sdjkflds"
        android:layout_below="@+id/child_text_view"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_medium" />

    <TextView
        android:id="@+id/bike_out"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_toRightOf="@id/icon_text_view"
        android:drawablePadding="@dimen/margin_16"
        android:gravity="center_vertical"
        android:text="sdjkflds"
        android:layout_below="@+id/child_text_view"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_medium" />



    <TextView
        android:id="@+id/car_in"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_toRightOf="@id/icon_text_view"
        android:drawablePadding="@dimen/margin_16"
        android:gravity="center_vertical"
        android:text="sdjkflds"
        android:layout_below="@+id/child_text_view"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_medium" />



    <TextView
        android:id="@+id/car_out"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_toRightOf="@id/icon_text_view"
        android:drawablePadding="@dimen/margin_16"
        android:gravity="center_vertical"
        android:text="sdjkflds"
        android:layout_below="@+id/child_text_view"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_medium" />


    <TextView
        android:id="@+id/car_collection"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_toRightOf="@id/icon_text_view"
        android:drawablePadding="@dimen/margin_16"
        android:gravity="center_vertical"
        android:text="sdjkflds"
        android:layout_below="@+id/child_text_view"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_medium" />


    <TextView
        android:id="@+id/bike_collection"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_toRightOf="@id/icon_text_view"
        android:drawablePadding="@dimen/margin_16"
        android:gravity="center_vertical"
        android:text="sdjkflds"
        android:layout_below="@+id/child_text_view"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_medium" />





    <TextView
        android:id="@+id/bike_present"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_toRightOf="@id/icon_text_view"
        android:drawablePadding="@dimen/margin_16"
        android:gravity="center_vertical"
        android:text="sdjkflds"
        android:layout_below="@+id/child_text_view"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_medium" />

    <TextView
        android:id="@+id/car_present"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_toRightOf="@id/icon_text_view"
        android:drawablePadding="@dimen/margin_16"
        android:gravity="center_vertical"
        android:text="sdjkflds"
        android:layout_below="@+id/child_text_view"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_medium" />


    <TextView
        android:id="@+id/total"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_toRightOf="@id/icon_text_view"
        android:drawablePadding="@dimen/margin_16"
        android:gravity="center_vertical"
        android:text="sdjkflds"
        android:layout_below="@+id/child_text_view"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_medium" />



</LinearLayout>